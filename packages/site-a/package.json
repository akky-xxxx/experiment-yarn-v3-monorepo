{
  "name": "site-a",
  "version": "0.0.1",
  "packageManager": "yarn@3.6.4",
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "check-all": "npm-run-all -s fix -p check-code -c",
    "check-code": "npm-run-all -p lint spell-check type-check -c",
    "dev": "next dev -p 3000",
    "fix": "run-p \"fix:*\" -c",
    "fix-ts-by-eslint": "eslint \"./{src,mock}/**/*.{ts,tsx}\" --fix --max-warnings 0 --cache",
    "fix-ts-by-prettier": "prettier \"./{src,mock}/**/*.{ts,tsx}\" --write",
    "fix:config": "prettier \"./.storybook/**/*\" \"./*.{js,mjs,cjs}\" --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-prettier fix-ts-by-eslint",
    "lint": "run-p \"lint:*\" -c",
    "lint-ts-by-eslint": "eslint \"./{src,mock}/**/*.{ts,tsx}\" --max-warnings 0 --cache",
    "lint-ts-by-prettier": "prettier \"./{src,mock}/**/*.{ts,tsx}\" --check",
    "lint:config": "prettier \"./.storybook/**/*\" \"./*.{js,mjs,cjs}\" --check",
    "lint:markup": "markuplint \"./src/**/*.{ts,tsx}\" -p",
    "lint:pack": "fixpack --dryRun",
    "lint:ts": "run-s lint-ts-by-prettier lint-ts-by-eslint",
    "prepare": "panda codegen",
    "spell-check": "cspell \"./{.storybook,config,src}/**\"",
    "start": "next start -p 3000",
    "storybook": "storybook dev -p 3010",
    "type-check": "tsc"
  },
  "dependencies": {
    "@experiment-yarn-v3-monorepo/common-components": "workspace:^",
    "jotai": "2.5.0",
    "next": "14.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@pandacss/dev": "0.17.5",
    "cspell": "7.3.8",
    "eslint": "8.53.0",
    "eslint-plugin-storybook": "0.6.15",
    "fixpack": "4.0.0",
    "jest": "29.7.0",
    "markuplint": "3.14.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.3",
    "storybook": "7.5.2"
  }
}
