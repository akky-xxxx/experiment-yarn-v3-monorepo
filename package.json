{
  "name": "experiment-yarn-v3-monorepo",
  "version": "0.0.1",
  "description": "experiment yarn monorepo",
  "author": "akky-xxxx <114.s.aki@gmail.com>",
  "repository": "https://github.com/akky-xxxx/experiment-yarn-v3-monorepo.git",
  "homepage": "https://github.com/akky-xxxx/experiment-yarn-v3-monorepo",
  "license": "MIT",
  "engines": {
    "node": "20.9.0",
    "npm": "⚠️️  【yarn】 を使ってください",
    "yarn": "4.0.1"
  },
  "packageManager": "yarn@4.0.1",
  "main": "index.js",
  "scripts": {
    "check-all": "npm-run-all -s fix -p check-code -c",
    "check-code": "npm-run-all -p lint spell-check -c",
    "fix": "run-p \"fix:*\" -c",
    "fix:config": "prettier \"./config/**/*\" \"./*.ts\" --write",
    "fix:pack": "fixpack",
    "lint": "run-p \"lint:*\" -c",
    "lint:config": "prettier \"./config/**/*\" --check",
    "lint:pack": "fixpack --dryRun",
    "prepare": "yarn workspaces foreach -A -vpi run prepare",
    "spell-check": "cspell \"./config/**\"",
    "ws-build": "yarn workspaces foreach -A -vpi run build",
    "ws-check-all": "yarn workspaces foreach -A -vpi run check-all",
    "ws-check-code": "yarn workspaces foreach -A -vpi run check-code",
    "ws-dev": "yarn workspaces foreach -A -vpi run dev",
    "ws-fix": "yarn workspaces foreach -A -vpi run fix",
    "ws-lint": "yarn workspaces foreach -A -vpi run lint",
    "ws-spell-check": "yarn workspaces foreach -A -vpi run spell-check",
    "ws-start": "yarn workspaces foreach -A -vpi run start",
    "ws-storybook": "yarn workspaces foreach -A -vpi run storybook",
    "ws-type-check": "yarn workspaces foreach -A -vpi run type-check"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "3.11.0",
    "@markuplint/react-spec": "3.12.0",
    "@storybook/addon-essentials": "7.5.2",
    "@storybook/addon-interactions": "7.5.2",
    "@storybook/addon-links": "7.5.2",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.5.2",
    "@storybook/nextjs": "7.5.2",
    "@storybook/react": "7.5.2",
    "@storybook/react-vite": "7.5.2",
    "@storybook/testing-library": "0.2.2",
    "@swc-jotai/debug-label": "0.1.0",
    "@swc-jotai/react-refresh": "0.1.0",
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "20.8.10",
    "@types/react": "18.2.34",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "cspell": "7.3.8",
    "eslint": "8.53.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-strict-check": "0.0.18",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-storybook": "0.6.15",
    "eslint-plugin-strict-check": "0.0.16",
    "eslint-plugin-strict-dependencies": "1.2.3",
    "eslint-plugin-unicorn": "49.0.0",
    "fixpack": "4.0.0",
    "jest": "29.7.0",
    "markuplint": "3.14.0",
    "msw": "1.3.1",
    "msw-storybook-addon": "1.10.0",
    "npm-run-all": "4.1.5",
    "openapi-typescript": "6.7.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "vite": "4.5.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "bugs": "https://github.com/akky-xxxx/experiment-yarn-v3-monorepo/issues",
  "keywords": [
    "Next.js",
    "monorepo",
    "yarn"
  ]
}
